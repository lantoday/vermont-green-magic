{
    "env": {
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "react-app",
        "react-app/jest",
        "airbnb-typescript",
        "prettier",
        "airbnb", 
        "airbnb/hooks"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": ["react"],
    "rules": {
        "react/prop-types": 0,
        "import/prefer-default-export": 0
    }
}
